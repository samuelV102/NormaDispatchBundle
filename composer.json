{
    "name": "norma-uy/template-symfony-bundle",
    "type": "symfony-bundle",
    "description": "Base template for building Symfony bundles",
    "keywords": [],
    "homepage": "https://github.com/norma-uy/TemplateSymfonyBundleBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Project Contributors",
            "homepage": "https://github.com/norma-uy/TemplateSymfonyBundle/graphs/contributors"
        }
    ],
    "require": {
        "php": ">=8.0.2",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/orm": "^2.10",
        "easycorp/easyadmin-bundle": "^4.7",
        "google/recaptcha": "^1.3",
        "league/flysystem-bundle": "^3.0",
        "liip/imagine-bundle": "^2.7",
        "symfony/config": "^5.4|^6.0",
        "symfony/dependency-injection": "^5.4|^6.0",
        "symfony/doctrine-bridge": "^5.4|^6.0",
        "symfony/event-dispatcher": "^5.4|^6.0",
        "symfony/filesystem": "^5.4|^6.0",
        "symfony/form": "^5.4|^6.0",
        "symfony/framework-bundle": "^5.4|^6.0",
        "symfony/http-foundation": "^5.4|^6.0",
        "symfony/http-kernel": "^5.4|^6.0",
        "symfony/intl": "^5.4|^6.0",
        "symfony/property-access": "^5.4|^6.0",
        "symfony/security-bundle": "^5.4|^6.0",
        "symfony/string": "^5.4|^6.0",
        "symfony/translation": "^5.4|^6.0",
        "symfony/twig-bundle": "^5.4|^6.0",
        "symfony/uid": "^5.4|^6.0",
        "symfony/validator": "^5.4|^6.0",
        "twig/twig": "^2.7|^3.0",
        "vich/uploader-bundle": "^2.0.1"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "phpstan/extension-installer": "^1.2",
        "phpstan/phpstan": "^1.9",
        "phpstan/phpstan-phpunit": "^1.2",
        "phpstan/phpstan-strict-rules": "^1.4",
        "phpstan/phpstan-symfony": "^1.2",
        "phpunit/phpunit": "^9.6",
        "psr/log": "^1.0",
        "symfony/browser-kit": "^5.4|^6.0",
        "symfony/css-selector": "^5.4|^6.0",
        "symfony/debug-bundle": "^5.4|^6.0",
        "symfony/dom-crawler": "^5.4|^6.0",
        "symfony/phpunit-bridge": "^5.4|^6.0",
        "symfony/stopwatch": "^5.4|^6.0",
        "symfony/test-pack": "^1.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "NormaUy\\Bundle\\TemplateSymfonyBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NormaUy\\Bundle\\TemplateSymfonyBundle\\Tests\\": "tests/",
            "NormaUy\\Bundle\\TemplateSymfonyBundle\\Tests\\TestApplication\\": "tests/TestApplication/src/"
        }
    }
}
